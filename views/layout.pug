		script.init(type = 'text/javascript').
			$(document).ready(function () {
				//var name = "#{location}".split(",");
				var location = "#{location_arr}";
				var loc_list = "#{location_arr}".split(",");
				var index = "#{index}";
				var test = "#{obj}";
				//menu(name);
				for(i=0; i<loc_list.length; i++) {
					if(i==index){
						create_menu(loc_list[i], i,loc_list,true,test);
					} else {
						create_menu(loc_list[i], i,loc_list,false);
					}
					
				}
			});
		script.init(type='text/javascript').
			$(document).ready(function() {
				$('#example').DataTable( {
					"pagingType": "full_numbers"
				} );
			});

		script(type='text/javascript').
			function poe_measure(){
				if(confirm("전류를 측정하시겠습니까?") == true){
				document.form.submit();
				setTimeout(function(){
					location.reload();
					},500);
				}
				else{
					return;
				}
				
			}
